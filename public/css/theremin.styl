body
	padding: 20px
	font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
	//background rgba(0,0,0,0.95);

#chart svg
	border 1px solid #999
line
	fill none
	stroke #000
	shape-rendering crispEdges
rect.key
	shape-rendering crispEdges
rect.black.key
	//width 80
	//height 13
	fill black
	stroke black
rect.white.key
	//width 120
	//height 23
	fill white
	stroke black
path.random
	stroke red
	fill none
	stroke-width 2px